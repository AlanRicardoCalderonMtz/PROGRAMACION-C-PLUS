//Tipos de datos en c++

#include <iostream>
using namespace std;

int main (){

    int numero = 15;
    float flotante = 10.45;
    double mayor = 16.3456;
    char letra = 'a';

    cout << "Entero: " << numero << endl;
    cout << "Flotante: " << flotante << endl;
    cout << "Double: " << mayor << endl;
    cout << "Char: " << letra << endl;

    return 0;
}